# API Configuration
# The backend API URL for authentication
# In production, this will be automatically set to the current origin
# In development, you can override it if your API is on a different port
VITE_API_URL=http://localhost:5173

# Farcaster Configuration
# Your deployed domain for JWT verification
# This should match your production domain
FARCASTER_DOMAIN=lapu.gg
VITE_CHAIN_ID=31337

# Base MiniKit Configuration
# Get your API key from https://portal.cdp.coinbase.com/
VITE_ONCHAINKIT_API_KEY=your_api_key_here

# CDP Project ID (optional, for additional features)
VITE_CDP_PROJECT_ID=your_project_id_here

# App Configuration
VITE_APP_NAME=Lapu
VITE_APP_URL=https://lapu.gg
VITE_APP_ICON_URL=https://lapu.gg/icons/gravity.webp
